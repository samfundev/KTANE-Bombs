{
	"arrowParens": "avoid",
	"bracketSameLine": true,
	"printWidth": 120,
	"semi": true,
	"singleAttributePerLine": false,
	"singleQuote": true,
	"trailingComma": "none",
	"useTabs": true,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
